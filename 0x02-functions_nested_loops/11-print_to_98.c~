#include<stdio.h>
#include "holberton.h"
/**
 * print_to_98 - check the code for Holberton School students.
 * @n: First operand
 * Return: Always 0.
 */
void print_to_98(int n)
{
int i;
if (n > 98)
{
for (i = n; i >= 98; i--)
{
printf("%d", i);
if (i != 98)
printf(", ");
}
}
if (n < 98)
{
for(i = n; i <= 98; i++)
{
printf("%d",i);
if(i != 98)
printf(", ");
}
}
if(n == 98)
printf("%d",n);
printf("\n");
}
