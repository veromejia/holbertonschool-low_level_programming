#include <stdio.h>
/**
 * main - Entry point
 *
 * Return: Always 0 (Success)
 */

int main(void)
{
int a;
int b;
int  c;
int j;
a = 1;
b = 2;
j = b;
do{
c = a + b;
a = b;
b = c;
if (c % 2 == 0)
j = j + c;
if (c <= 4000000)
continue;
else
break;
}while (c < 4000000);
printf("%d\n", j);
return (0);
}
