#include <stdio.h>
#include "holberton.h"
/**
 * mul - function that multiplies two integers.
 * @a: First operand
 * @b: Second operand
 * Description: multiplies 2 integers positives or negatives
 * Return: the product of the 2 operands
 *
 */
int mul(int a, int b)
{
  int result;
  result = a * b;
  return (result);
}
